{
  "expo": {
    "name": "LuxAI Chat",
    "slug": "rag-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon.png",
    "scheme": "ragchat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.luxai.ragchat"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#fff",
          "image": "./assets/images/splash-icon-lightmode.png",
          "dark": {
            "image": "./assets/images/splash-icon-darkmode.png",
            "backgroundColor": "#151718"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f1a5c265-00fc-4ff7-8693-776a89435678"
      }
    }
  }
}
